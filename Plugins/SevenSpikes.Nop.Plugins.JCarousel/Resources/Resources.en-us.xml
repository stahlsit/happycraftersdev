        <Children>
          <LocaleResource Name="Admin">
            <Value>Admin</Value>
            <Children>
              <LocaleResource Name="Settings">
                <Value>Settings</Value>
                <Children>
                  <LocaleResource Name="Common">
                    <Value>Nop JCarousel Settings</Value>
                  </LocaleResource>
                  <LocaleResource Name="CarouselOrientation">
                    <Value>Rotate the JCarousel to vertical position (default: horizontal)</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specifies wether the carousel appears in horizontal or vertical orientation. Changes the carousel from a left/right style to a up/down style carousel.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="RightToLeft">
                    <Value>Enable right to left mode</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specifies wether the carousel appears in RTL (Right-To-Left) mode.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="StartIndex">
                    <Value>Item start index</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>The index of the item to start with.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ScrollItems">
                    <Value>Number of items to scroll by</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>The number of items to scroll by.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="AnimationSpeed">
                    <Value>The speed of the scroll animation</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>The speed of the scroll animation in terms ("none", "slow" or "fast"). If set to "none", animation is turned off.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="Easing">
                    <Value>Easing effect</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>The name of the easing effect that you want to use.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="AdvancedSettings">
                    <Value>Advanced Settings</Value>
                  </LocaleResource>

                  <LocaleResource Name="Skin">
                    <Value>Skin</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specufies the skin of the JCarousel.</Value>
                      </LocaleResource>
                      <LocaleResource Name="Required">
                        <Value>Carousel Skin is REQUIRED.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="Autoscroll">
                    <Value>Autoscroll the content after (seconds)</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specifies after how many seconds to periodically autoscroll the content. If set to 0 (default) then autoscrolling is turned off.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ImageSize">
                    <Value>Image size (pixels)</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specifies a custom JCarousel image size. If set to 0 the image size will be the default nopcommerce picture tumbnail image size.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="WrapItems">
                    <Value>Wrap items</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specifies whether to wrap at the first/last item (or both) and jump back to the start/end. Options are "none", "first", "last", "both" or "circular". If set to "none", wrapping is turned off (default).</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="DataSourceType">
                    <Value>Data source type</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specifies predefined type of data that will populate the JCarousel plugin e.g. "Featured Products", "Related Products", etc.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="NumberOfItems">
                    <Value>Maximum number of items</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>The maximum number of items that will contain the JCarousel.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ShowShortDescription">
                    <Value>Show short description</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>If checked the short description will be shown in the JCarousel.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ShowDetailsButton">
                    <Value>Show details button</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>If checked a details button will be shown in the JCarousel.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="Name">
                    <Value>Name</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>Specifies the name of the JCarousel.</Value>
                      </LocaleResource>
                      <LocaleResource Name="Required">
                        <Value>Name of the JCarousel is REQUIRED.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ShowItemsName">
                    <Value>Show item name</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>If checked the names of the products will be shown in the JCarousel.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ShowTitle">
                    <Value>Show title</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>If checked the title of the JCarpisel will be shown.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ShowProdictsPrice">
                    <Value>Show product price</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>If checked the price of the products will be shown in the JCarousel.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="ShowProdictsOldPrice">
                    <Value>Show product old price</Value>
                    <Children>
                      <LocaleResource Name="Hint">
                        <Value>If checked the old price of the products will be shown in the JCarousel.</Value>
                      </LocaleResource>
                    </Children>
                  </LocaleResource>
                  <LocaleResource Name="Title">
